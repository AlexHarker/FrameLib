<c74object category="FrameLib Routing" module="FrameLib" name="fl.select~">
  <digest> 
        Selects output from a number of inputs.
    </digest>
  <description> 
         The number of inputs is variable. The selected input can be changed with a parameter.
    </description>
  <misc name="Parameters">
    <entry name="1. /num_ins [instantiation double]">
      <description> 
                Sets the number of inputs.
            </description>
    </entry>
    <entry name="2. /input [int]">
      <description> 
                Sets the current input counting from 1 (or off if out of range).
            </description>
    </entry>
  </misc>
  <metadatalist>
    <metadata name="author">Alex Harker</metadata>
    <metadata name="tag">FrameLib</metadata>
    <metadata name="tag">FrameLib Routing</metadata>
  </metadatalist>
  <seealsolist>
    <seealso name="fl.dispatch~" />
    <seealso name="fl.route~" />
  </seealsolist>
  <misc name="Discussion">
    <entry name="Keywords">
      <description>routing</description>
    </entry>
  </misc>
</c74object>
