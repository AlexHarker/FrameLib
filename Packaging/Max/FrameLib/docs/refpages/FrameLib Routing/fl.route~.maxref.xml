<c74object name="fl.route~" module="FrameLib" category="FrameLib Routing">
  <digest> 
        Routes the input to one of a number of outputs.
    </digest>
  <description> 
         The number of outputs is variable. The selected output can be changed with a parameter.
    </description>
  <misc name="Parameters">
    <entry name="/num_outs [instantiation double]">
      <description> 
                Sets the number of outputs.
            </description>
    </entry>
    <entry name="/output [int]">
      <description> 
                Sets the current output counting from 1 (off if out of range).
            </description>
    </entry>
  </misc>
  <metadatalist>
    <metadata name="author">Alex Harker</metadata>
    <metadata name="tag">FrameLib</metadata>
    <metadata name="tag">FrameLib Routing</metadata>
  </metadatalist>
  <objarglist>
    <objarg name="num_outs" optional="1" type="number">
      <digest> 
                Sets the number of outputs
            </digest>
      <description> 
                Sets the number of outputs.
            </description>
    </objarg>
    <objarg name="output" optional="1" type="int">
      <digest> 
                Sets the current output counting from 1 (off if out of range)
            </digest>
      <description> 
                Sets the current output counting from 1 (off if out of range).
            </description>
    </objarg>
  </objarglist>
