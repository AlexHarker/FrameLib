<c74object name="fl.pack~" module="FrameLib" category="FrameLib Streaming">
  <digest> 
        Packs multiple frame streams into a multistream output.
    </digest>
  <description> 
         Inputs may be single stream or multistream. All inputs are concatenated in order, with blank inputs ignored.
    </description>
  <misc name="Parameters">
    <entry name="1. /num_ins [instantiation int]">
      <description> 
                Sets the number of inputs.
            </description>
    </entry>
  </misc>
  <metadatalist>
    <metadata name="author">Alex Harker</metadata>
    <metadata name="tag">FrameLib</metadata>
    <metadata name="tag">FrameLib Streaming</metadata>
  </metadatalist>
  <objarglist>
    <objarg name="num_ins" optional="1" type="int">
      <digest> 
                Sets the number of inputs
            </digest>
      <description> 
                Sets the number of inputs.
            </description>
    </objarg>
  </objarglist>
  <attributelist>
    <attribute name="rt" get="1" set="1" type="int" size="1">
      <digest> 
