<c74object name="fl.join~" module="FrameLib" category="FrameLib Vector">
  <digest>
    Concatenates all input frames into a single output frame.
  </digest>
  <description>
    Each input may trigger output (or not), as set by the triggers_in parameter.
  </description>
  <misc name="Parameters">
    <entry name="/num_ins [instantiation int]">
      <description>
        Sets the number of inputs.
      </description>
    </entry>
    <entry name="/trigger_ins [instantiation fixed length bool array]">
      <description>
        Sets which inputs trigger output using an array of bools.<br/>By default all inputs trigger output.
      </description>
    </entry>
  </misc>
  <metadatalist>
    <metadata name="author">Alex Harker</metadata>
    <metadata name="tag">FrameLib</metadata>
    <metadata name="tag">FrameLib Vector</metadata>
  </metadatalist>
  <objarglist>
    <objarg name="num_ins" optional="1" type="int">
      <digest>
        Sets the number of inputs
      </digest>
      <description>
        Sets the number of inputs.
      </description>
    </objarg>
  </objarglist>
