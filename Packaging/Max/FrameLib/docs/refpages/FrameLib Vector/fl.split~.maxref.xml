<c74object name="fl.split~" module="FrameLib" category="FrameLib Vector">
  <digest> 
        Splits input frames into two frames, given a specified split point.
    </digest>
  <description> 
         The split point may be specified in samples or as a ratio of the length of the input frame.
    </description>
  <misc name="Parameters">
    <entry name="1. /split [double]">
      <description> 
                Sets the split point.
            </description>
    </entry>
    <entry name="2. /units [enum]">
      <description> 
                Sets the units for the split point (samples or ratios [0-1]).<br />
        <bullet>[0] - samples</bullet>
        <bullet>[1] - ratios</bullet>
      </description>
    </entry>
  </misc>
  <metadatalist>
    <metadata name="author">Alex Harker</metadata>
    <metadata name="tag">FrameLib</metadata>
    <metadata name="tag">FrameLib Vector</metadata>
  </metadatalist>
  <objarglist>
    <objarg name="split" optional="1" type="number">
      <digest> 
                Sets the split point
            </digest>
      <description> 
                Sets the split point.
            </description>
    </objarg>
    <objarg name="units" optional="1" type="symbol">
      <digest> 
                Sets the units for the split point (samples or ratios [0-1])
            </digest>
      <description> 
                Sets the units for the split point (samples or ratios [0-1]).
            </description>
    </objarg>
  </objarglist>
