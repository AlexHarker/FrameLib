<c74object name="fl.store~" module="FrameLib" category="storage">

	<digest>
		Stores a vector frame in named memory for recall.
	</digest>

	<description>
		The output can be used to control ordering/synchronisation.
	</description>

	<misc name = "Parameters">
		<entry name = "1. /name [string]">
			<description>
				Sets the name of the memory location to use.
			</description>
		</entry>
	</misc>

	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Alex Harker</metadata>
		<metadata name="tag">FrameLib</metadata>
		<metadata name="tag">Storage</metadata>
	</metadatalist>

