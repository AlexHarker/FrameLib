<c74object name="fl.register~" module="FrameLib" category="storage">

	<digest>
		Store and recall a vector frame locally.
	</digest>

	<description>
		The left input triggers recall, whilst the right input stores (with or without output).
	</description>

	<misc name = "Parameters">
		<entry name = "1. /mode [enum]">
			<description>
				Sets the behaviour of the right input:<br></br>
				<bullet><b>[0] - store:</b> Stores without output</bullet><br></br>
				<bullet><b>[1] - pass:</b> Stores and outputs</bullet><br></br>
			</description>
		</entry>
	</misc>

	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Alex Harker</metadata>
		<metadata name="tag">FrameLib</metadata>
		<metadata name="tag">Storage</metadata>
	</metadatalist>

	<!--SEEALSO-->
	<!-- The information here is pulled from the digest of the corresponding object-->
	<seealsolist>
		<seealso name="fl.recall~" />
		<seealso name="fl.store~" />
	</seealsolist>

	<misc name = "Discussion">
		<entry name ="Keywords">
			<description>
				storage, buffer, value, <o>zl</o>
			</description>
		</entry>
	</misc>

</c74object>