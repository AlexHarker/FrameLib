<c74object category="FrameLib Mapping" module="FrameLib" name="fl.constant~">
  <digest> 
        Outputs a specified constant.
    </digest>
  <description> 
         Output is a single value. The constant can be multiplied before output as set by the multiplier parameter.
    </description>
  <misc name="Parameters">
    <entry name="1. /constant [enum]">
      <description> 
                Sets the constant to output.<br />
        <bullet>[0] - pi</bullet>
        <bullet>[1] - 1/pi</bullet>
        <bullet>[2] - e</bullet>
        <bullet>[3] - 1/e</bullet>
        <bullet>[4] - sqrt2</bullet>
        <bullet>[5] - 1/sqrt2</bullet>
        <bullet>[6] - epsilon</bullet>
      </description>
    </entry>
    <entry name="2. /multiplier [double]">
      <description> 
                Sets the multiplier.
            </description>
    </entry>
  </misc>
  <metadatalist>
    <metadata name="author">Alex Harker</metadata>
    <metadata name="tag">FrameLib</metadata>
    <metadata name="tag">FrameLib Mapping</metadata>
  </metadatalist>
  <seealsolist>
    <seealso name="fl.convert~" />
    <seealso name="fl.map~" />
    <seealso name="fl.samplerate~" />
    <seealso name="fl.lookup~" />
  </seealsolist>
  <misc name="Discussion">
    <entry name="Keywords">
      <description>scaling,mapping</description>
    </entry>
  </misc>
</c74object>
