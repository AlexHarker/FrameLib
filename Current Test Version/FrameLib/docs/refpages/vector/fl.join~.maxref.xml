<c74object name="fl.join~" module="FrameLib" category="vector">

	<digest>
		Concatenates all input frames into a single output frame.
	</digest>

	<description>
		Inputs can be set to trigger output or not.
	</description>

	<misc name = "Parameters">
		<entry name = "1. /num_ins [int]">
			<description>
				Sets the number of inputs [2-32].
			</description>
		</entry>

		<entry name = "2. /trigger_ins [bool array]">
			<description>
				Sets which inputs trigger output by default all inputs.
			</description>
		</entry>
	</misc>

	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Alex Harker</metadata>
