<c74object category="Mapping" module="FrameLib" name="fl.constant~"> 
 
    <digest> 
        Outputs the specified useful constant.
    </digest> 
 
    <description> 
         Output is a single value. The constant can be multiplied before output
    </description> 
 
    <misc name="Parameters"> 
 
        <entry name="0. /constant [enum]"> 
            <description> 
                Sets the constant to output.<br /> 
                <bullet>[0] - pi</bullet> 
                <bullet>[1] - e</bullet> 
                <bullet>[2] - sqrt2</bullet> 
                <bullet>[3] - 1/pi</bullet> 
                <bullet>[4] - 1/e</bullet> 
                <bullet>[5] - 1/sqrt2</bullet> 
                <bullet>[6] - epsilon</bullet>
            </description> 
        </entry> 
 
        <entry name="1. /multiplier [double]"> 
            <description> 
                Sets the multiplier.
            </description> 
        </entry> 
 
    </misc> 
 
    <metadatalist> 
        <metadata name="author">Alex Harker</metadata> 
        <metadata name="tag">FrameLib</metadata> 
        <metadata name="tag">Mapping</metadata> 
    </metadatalist> 
 
