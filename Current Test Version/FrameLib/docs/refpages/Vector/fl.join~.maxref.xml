<c74object category="Vector" module="FrameLib" name="fl.join~"> 
 
    <digest> 
        Concatenates all input frames into a single output frame.
    </digest> 
 
    <description> 
         Inputs can be set to trigger output or not.
    </description> 
 
    <misc name="Parameters"> 
 
        <entry name="0. /num_ins [instantiation int]"> 
            <description> 
                Sets the number of inputs.
            </description> 
        </entry> 
 
        <entry name="1. /trigger_ins [instantiation fixed length bool array]"> 
            <description> 
                Set which inputs trigger output (by default all inputs).
            </description> 
        </entry> 
 
    </misc> 
 
    <metadatalist> 
        <metadata name="author">Alex Harker</metadata> 
        <metadata name="tag">FrameLib</metadata> 
        <metadata name="tag">Vector</metadata> 
    </metadatalist> 
 
